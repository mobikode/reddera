
<section class="bg">
  <div class="carousel slide " id="slideshow-carousel-1" data-ride="carousel" data-interval="7000" style="z-index: 1000">  <!-- Indicators -->
    <div class="container">
      <div class="header-content search-form-pos text-center" >
        <p class="ht-title">
          Be a star, beauty and wellness delivered to you
          <!--<span>A new</span>-->
          <!--<span class="heart-icon-2"><img src="<%#=asset_path('era.png')%>"></span>-->
          <!--<span>in beauty and wellness.</span>-->
        </p>
        <div class="form-section headerFrom">
          <%= form_tag search_path, :method => :get do %>

              <div class="row">
                <form role="form">
                  <div class="col-md-4 col-xs-12 no-pad">
                    <div class="form-group custom-style">

                      <div class="typeahead__container">
                        <div class="typeahead__field">

            <span class="typeahead__query">



              <%= text_field_tag :name, params[:name], class: "js-typeahead-country_name form-control no-border form-radius-left", placeholder: "Search a Professional or Service", autocomplete:"off"%>
            </span>


                        </div>
                      </div>
                    </div>

                  </div>
                  <div class="col-md-2 col-xs-12 no-pad">
                    <div class="form-group custom-style">
                      <label class="sr-only" for="location">Location</label>

                      <div class="form-group custom-style">
                        <div class="typeahead__container">
                          <div class="typeahead__field">

                        <span class="typeahead__query_location">


                          <%= text_field_tag :location, params[:location], class: "js-typeahead-country_v1_location5 form-control no-border ", placeholder: "Location", autocomplete:"off"%>
                        </span>


                          </div>
                        </div>
                      </div>
                    </div>
                  </div>
                  <div class="col-md-2 col-xs-12 no-pad">
                    <div class="form-group custom-style">
                      <label class="sr-only" for="checkin">Date</label>

                      <div class="input-group">
                        <%= text_field_tag :date, params[:date], class: "form-control no-border datepickersearch ", placeholder: "Date", autocomplete:"off",id: 'datepickersearch' %>
                      </div>
                    </div>
                  </div>
                  <div class="col-md-2 col-xs-12 no-pad">
                    <div class="form-group custom-style">
                      <label class="sr-only" for="checkout">Time</label>
                      <%#debugger%>
                      <%#= select_tag("time", options_from_collection_for_select(@availabilities, "value", "key" ), class: 'form-control')%>
                      <select name="time" id="time" class="form-control" style=" "><option value="">Anytime</option><option value="06AM, 07AM, 08AM">6am-8am</option>
                        <option value="09AM, 10AM, 11AM">9am-11am</option>
                        <option value="12PM, 01PM, 02PM, 03PM, 04PM">12pm-4pm</option>
                        <option value="05PM, 06PM, 07PM, 08PM">5pm-8pm</option>
                        <option value="09PM, 10PM, 11PM">9pm-11pm</option></select>                           </div>
                  </div>
                  <div class="col-md-2 col-xs-12 no-pad">
                    <%= submit_tag "Search", class: "btn btn-default red-btn btn-radius" %>
                  </div>
                </form>
              </div>
          <% end %>
        </div>
      </div>
    </div>
    <div class="carousel-inner" role="listbox">

    <div class="item active">
      <div class="slider1">

      </div>
    </div>
    <div class="item">
      <div class="slider2">

      </div>
    </div>
    <div class="item">
      <div class="slider3">

      </div>
    </div>
    <div class="item">
      <div class="slider4">

      </div>
    </div>
    <div class="item">
      <div class="slider5">

      </div>
    </div>


  </div>
  <!--<a class="left carousel-control" href="#slideshow-carousel-1" role="button" data-slide="prev"> <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span> <span class="sr-only">Previous</span> </a> <a class="right carousel-control" href="#slideshow-carousel-1" role="button" data-slide="next"> <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span> <span class="sr-only">Next</span> </a> -->
  </div>
</section>

